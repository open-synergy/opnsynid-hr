<openerp>
<data>
<record id="base_sequence_promotion" model="base.sequence_configurator">
    <field name="model_id" ref="hr_promotion_transition.model_hr_promotion_transition"/>
    <field name="sequence_field_id" search="[('model_id.model','=','hr.promotion_transition'),('name','=','name')]"/>
    <field name="fallback_sequence_id" ref="hr_promotion_transition.sequence_promotion_transition"/>
</record>

<record id="base_sequence_promotion_line" model="base.sequence_configurator_line">
    <field name="generator_id" ref="hr_promotion_transition.base_sequence_promotion"/>
    <field name="sequence" eval="1"/>
    <field name="sequence_computation_code">result = document.type_id.sequence_id</field>
</record>
</data>
</openerp>
