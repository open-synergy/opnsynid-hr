<openerp>
    <data>
        <!-- HR Overtime Request -->
        <record
            id="base_sequence_hr_overtime_request"
            model="base.sequence_configurator"
        >
            <field
                name="model_id"
                ref="hr_attendance_overtime_request.model_hr_overtime_request"
            />
            <field
                name="sequence_field_id"
                search="[('model_id.model','=','hr.overtime_request'),('name','=','name')]"
            />
            <field
                name="fallback_sequence_id"
                ref="hr_attendance_overtime_request.sequence_overtime_request"
            />
        </record>

        <record
            id="base_sequence_hr_overtime_request_line"
            model="base.sequence_configurator_line"
        >
            <field name="generator_id" ref="base_sequence_hr_overtime_request" />
            <field name="sequence" eval="1" />
            <field
                name="sequence_computation_code"
            >result = document.company_id.overtime_request_sequence_id</field>
        </record>
    </data>
</openerp>
