<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data>

<record id="hr_payslip_view_form" model="ir.ui.view">
    <field name="name">hr.payslip form</field>
    <field name="model">hr.payslip</field>
    <field name="inherit_id" ref="hr_payroll.view_hr_payslip_form"/>
    <field name="arch" type="xml">
        <xpath expr="//notebook/page[1]" position="after">
            <page string="Timesheet Computation">
                <separator name="timesheet_computation_summary" string="Timesheet Computation Summary"/>
                <field name="timesheet_computation_summary_ids" colspan="4" nolabel="1">
                    <tree>
                        <field name="item_id"/>
                        <field name="amount"/>
                    </tree>
                </field>
                <separator name="timesheet_computation_details" string="Timesheet Computation Details"/>
                <field name="timesheet_computation_ids" colspan="4" nolabel="1">
                    <tree>
                        <field name="sheet_id"/>
                        <field name="item_id"/>
                        <field name="amount"/>
                    </tree>
                </field>
            </page>
        </xpath>
    </field>
</record>

</data>
</openerp>
