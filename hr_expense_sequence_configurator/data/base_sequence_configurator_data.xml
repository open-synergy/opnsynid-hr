<openerp>
<data>
<!-- Training -->
<record id="base_sequence_hr_expense" model="base.sequence_configurator">
    <field name="model_id" ref="hr_expense.model_hr_expense_expense" />
    <field
                name="sequence_field_id"
                search="[('model_id.model','=','hr.expense.expense'),('name','=','name')]"
            />
    <field
                name="fallback_sequence_id"
                ref="hr_expense_sequence_configurator.sequence_reimbusement"
            />
</record>

<record id="base_sequence_hr_expense_line" model="base.sequence_configurator_line">
    <field
                name="generator_id"
                ref="hr_expense_sequence_configurator.base_sequence_hr_expense"
            />
    <field name="sequence" eval="1" />
    <field
                name="sequence_computation_code"
            >result = document.journal_id.sequence_id</field>
</record>
</data>
</openerp>
