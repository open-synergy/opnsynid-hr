<openerp>
<data>
<record id="base_sequence_join" model="base.sequence_configurator">
    <field name="model_id" ref="hr_join_transition.model_hr_join_transition"/>
    <field name="sequence_field_id" search="[('model_id.model','=','hr.join_transition'),('name','=','name')]"/>
    <field name="fallback_sequence_id" ref="hr_join_transition.sequence_join_transition"/>
</record>

<record id="base_sequence_join_line" model="base.sequence_configurator_line">
    <field name="generator_id" ref="hr_join_transition.base_sequence_join"/>
    <field name="sequence" eval="1"/>
    <field name="sequence_computation_code">result = document.type_id.sequence_id</field>
</record>
</data>
</openerp>
