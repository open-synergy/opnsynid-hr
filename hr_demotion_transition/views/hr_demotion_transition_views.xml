<openerp>
<data>
<record id="hr_demotion_transition_view_form" model="ir.ui.view">
    <field name="name">hr.demotion_transition form</field>
    <field name="model">hr.demotion_transition</field>
    <field name="inherit_id" ref="hr_career_transition.hr_career_transition_view_form"/>
    <field name="mode">primary</field>
    <field name="arch" type="xml">
        <data/>
    </field>
</record>

<record id="hr_demotion_transition_view_tree" model="ir.ui.view">
    <field name="name">hr.demotion_transition tree</field>
    <field name="model">hr.demotion_transition</field>
    <field name="inherit_id" ref="hr_career_transition.hr_career_transition_view_tree"/>
    <field name="mode">primary</field>
    <field name="arch" type="xml">
        <data/>
    </field>
</record>

<record id="hr_demotion_transition_view_search" model="ir.ui.view">
    <field name="name">hr.demotion_transition search</field>
    <field name="model">hr.demotion_transition</field>
    <field name="inherit_id" ref="hr_career_transition.hr_career_transition_view_search"/>
    <field name="mode">primary</field>
    <field name="arch" type="xml">
        <data/>
    </field>
</record>

<record id="hr_demotion_transition_action" model="ir.actions.act_window">
    <field name="name">Demotions</field>
    <field name="res_model">hr.demotion_transition</field>
    <field name="view_type">form</field>
    <field name="view_mode">tree,form</field>
    <field name="context">{'search_default_filter_draft':1, 'search_default_filter_confirm':1, 'search_default_filter_open':1, 'readonly_by_pass': True}</field>
    <field name="search_view_id" ref="hr_demotion_transition_view_search"/>
</record>

<record id="hr_demotion_transition_window_tree" model="ir.actions.act_window.view">
    <field eval="1" name="sequence"/>
    <field name="view_mode">tree</field>
    <field name="view_id" ref="hr_demotion_transition_view_tree"/>
    <field name="act_window_id" ref="hr_demotion_transition_action"/>
</record>

<record id="hr_demotion_transition_window_form" model="ir.actions.act_window.view">
    <field eval="2" name="sequence"/>
    <field name="view_mode">form</field>
    <field name="view_id" ref="hr_demotion_transition_view_form"/>
    <field name="act_window_id" ref="hr_demotion_transition_action"/>
</record>

<menuitem
    id="hr_demotion_transition_menu"
    name="Demotions"
    parent="hr_career_transition.hr_career_transition_root_menu"
    action="hr_demotion_transition_action"
    sequence="22"/>

</data>
</openerp>
